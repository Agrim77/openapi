<%- include('./partials/header') %>

<div class="page2">
  <div class="dash_section1">
    <div class="dashboard_intro">
      <h2>My dashboard</h2>
    </div>

    <div class="dashboard_search">
      <div class="search-bar">
        <input
          type="text"
          id="search-input"
          placeholder="What are you looking for?"
        />
        <button id="search-button">
          <i class="fas fa-search"></i>
        </button>
      </div>
    </div>
  </div>

  <div class="dash_section2">
    <div class="user_dash">
      <div class="user_content">
        <h1>Good Evening User !</h1>
        <br />
        <p>A simple quote of the day !</p>
        <div id="paypal-button-container"></div>
      </div>
      <div class="user_img">
        <img src="/vector.png" alt="" />
      </div>
    </div>
    <div class="user_nav">
      <h2>50 Questions</h2>
      <p>SLIDER HERE !</p>
    </div>
  </div>

  <div class="dash_section3">
    <h4>Hi User</h4>
    <br />
    <h1>We are here to help</h1>
    <br />
    <p>
      Welcome to the expert response to the 50 top
      <br />
      interview questions tailored to you !
    </p>
  </div>

  <div class="dash_section4">
    <h2>50 Top Interview Questions</h2>
    <h2>Result length:<%= result.length %></h2>
    <p>Everything you need to know to ace your interviews!</p>
    <div class="qs">
        <% for (let i = 0; i < result.length; i++) { %>
            <h3>Question <%= i+1 %>: <%= result[i].ques %></h3>
            <!-- <pre><%- unescape(result[i].ans.replace(/\r?\n|\r/g, '<br>')) %></pre> -->
            <p>Answer <%= i+1 %>:</p>
            <pre><%-result[i].ans %></pre>
            <br>
            Using pre and code syntax
            <pre><code><%- unescape(result[i].ans.replace(/<br>/g, '\n').replace(/&lt;/g, '<').replace(/&gt;/g, '>').replace(/&quot;/g, '"').replace(/&amp;/g, '&').replace(/&nbsp;/g, ' ').replace(/</g, '&lt;').replace(/>/g, '&gt;')) %></code></pre>

          <% } %>
    </div>
  </div>

  <div class="dash_section5">
    <div id="paypal-button-container"></div>
  </div>
</div>

<script src="https://www.paypal.com/sdk/js?client-id=AczF_NZmU4pVaN9FeL8WOj9St9782qUfdpWG3hkta_xgod5LMXYFt02FZsl7N0dF_hx3n4_ydnAs3Uoy&currency=USD"></script>
    <!-- Set up a container element for the button -->


<script src=".paypal_checkout.js"></script>

<!-- <%- include('./partials/footer') %> -->
</body>
</html>
